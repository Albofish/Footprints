{
    "name": "albofish/footstep",
    "type": "library",
    "description": "A simple registration attribution tracking solution for Laravel 5.2+ (UTM Parameters and Referrers)",
    "keywords": [
        "footprints",
        "tracking",
        "utm",
        "utm parameters",
        "user tracking",
        "attribution tracking",
        "footstep"
    ],
    "homepage": "https://github.com/albofish/footstep",
    "license": "MIT",
    "authors": [
        {
            "name": "Kyran Beasley",
            "email": "kyranbeasley@gmail.com"
        },
        {
            "name": "Alex Hollyman",
            "email": "alex.hollyman@gmail.com"
        }
    ],
    "require": {
        "illuminate/support": "~5.2|~5.3",
        "php" : "~5.5|~7.0"
    },
    "require-dev": {
        "laravel/framework": "5.2",
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "Albofish\\Footstep\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Albofish\\Footstep\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
